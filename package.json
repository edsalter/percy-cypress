{
  "name": "@percy/cypress",
  "version": "2.3.1",
  "description": "Cypress client library for visual regression testing with Percy (https://percy.io).",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "start-server-and-test server:start 8000 cypress:percy",
    "server:start": "http-server --silent --port 8000 cypress/testapp",
    "cypress:percy": "percy exec -- cypress run",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/percy/percy-cypress.git"
  },
  "keywords": [
    "cypress"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Perceptual Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/percy/percy-cypress/issues"
  },
  "homepage": "https://github.com/percy/percy-cypress#readme",
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.12",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "cypress": "^5.1.0",
    "http-server": "^0.12.3",
    "semantic-release": "^17.0.4",
    "start-server-and-test": "^1.11.4"
  },
  "peerDependencies": {
    "@percy/cli": "^1",
    "cypress": "^3 || ^4 || ^5"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
